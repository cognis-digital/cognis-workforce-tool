{
  "name": "cognis-digital-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:reset": "npx supabase db reset",
    "supabase:status": "npx supabase status",
    "supabase:functions:deploy": "npx supabase functions deploy",
    "supabase:functions:serve": "npx supabase functions serve",
    "contracts:compile": "npx hardhat compile",
    "contracts:deploy": "node scripts/deploy.js",
    "contracts:deploy-all": "npm run contracts:deploy -- --network sepolia && npm run contracts:deploy -- --network mumbai && npm run contracts:deploy -- --network bscTestnet && npm run contracts:deploy -- --network fuji && npm run contracts:deploy -- --network arbitrumSepolia",
    "ai:dev": "npx nodemon --watch server --exec \"npx ts-node server/index.ts\"",
    "ai:start": "npx ts-node server/index.ts",
    "setup": "npm run supabase:start && npm run supabase:functions:deploy && npm run contracts:compile",
    "postinstall": "node -e \"console.log('Skipping postinstall audit fix')\"",
    "cognis:dev": "./start-dev.sh",
    "cognis:server": "NODE_ENV=development npx nodemon --watch server --exec \"node cognis-server.js\"",
    "cognis:build": "npm run build && npx tsc -p tsconfig.server.json",
    "cognis:deploy": "./deploy-production.sh",
    "cognis:start": "NODE_ENV=production node cognis-server.js",
    "cognis:api": "node start-cognis-api.js",
    "cognis:api:dev": "npx nodemon --watch start-cognis-api.js --exec \"node start-cognis-api.js\""
  },
