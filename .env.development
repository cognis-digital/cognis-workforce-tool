# Development environment configuration for Cognis Workforce Tool

# Server configuration
PORT=3000

# Backend type: 'local', 'openai', or 'cognis'
BACKEND=cognis

# Cognis API configuration
COGNIS_API_KEY=sk-cognis-workforce-prod-valid-key-45678
VITE_COGNIS_API_KEY=sk-cognis-workforce-prod-valid-key-45678
COGNIS_API_URL=https://api.cognisdigital.com/v1
VITE_API_URL=https://api.cognisdigital.com/v1
COGNIS_DEFAULT_MODEL=Cognis-Zenith-4.0
VITE_COGNIS_DEFAULT_MODEL=Cognis-Zenith-4.0

# Optional OpenAI configuration (fallback)
OPENAI_API_KEY=

# Local model settings (when using 'local' backend)
MODEL_ID=Xenova/TinyLlama-1.1B-Chat-v1.0
MAX_TOKENS=384
TEMP=0.7
WASM_THREADS=4
CTX_WINDOW=1536
MODEL_CACHE_DIR=.model-cache

# Feature flags
FEATURE_TEMPORAL_SNAPSHOTS=true
FEATURE_MULTI_AGENT=true
