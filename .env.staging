# Cognis Evolution Architecture - Staging Environment

# API Configuration
API_URL=https://staging-api.cognisdigital.com
API_VERSION=v1
API_TIMEOUT=30000

# Evolution Architecture Settings
EVOLUTION_HISTORY_LIMIT=100
EVOLUTION_AUTO_SNAPSHOT=true
POLYMORPHIC_GENERATION_ENABLED=true

# Cognis AI Model Configuration
COGNIS_MODEL_ENDPOINT=https://staging-models.cognisdigital.com
COGNIS_ZENITH_VERSION=4.0
COGNIS_APEX_VERSION=3.5
COGNIS_VERTEX_VERSION=4.0
COGNIS_MODEL_TIMEOUT=60000

# Authentication
AUTH_DOMAIN=staging-auth.cognisdigital.com
AUTH_CLIENT_ID=staging-client-id
AUTH_AUDIENCE=staging-audience

# Blockchain Integration
BLOCKCHAIN_ENABLED=true
BLOCKCHAIN_PROVIDER_URL=https://ethereum-staging.infura.io/v3/staging-key
SMART_CONTRACT_ADDRESS=0xStaging0000000000000000000000000000000000

# Feature Flags
FEATURE_TEMPORAL_SNAPSHOTS=true
FEATURE_MULTI_AGENT=true
FEATURE_BLOCKCHAIN_VERIFICATION=true

# Client-side exposed variables (prefixed with VITE_)
VITE_APP_NAME=Cognis Digital - Staging
VITE_API_URL=https://staging-api.cognisdigital.com
VITE_APP_VERSION=staging
